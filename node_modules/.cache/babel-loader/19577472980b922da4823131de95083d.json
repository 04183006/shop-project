sSelectAll) {\r\n          this.$store.state.cartList.forEach(item => item.checked = false)\r\n        } else {\r\n          this.$store.state.cartList.forEach(item => item.checked = true)\r\n        }\r\n      },\r\n      calcClick() {\r\n        if (!this.isSelectAll) {\r\n          this.$toast.show('请选择购买的商品', 2000);\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n  .bottom-bar {\r\n    height: 40px;\r\n    background-color: #eee;\r\n    position: relative;\r\n    line-height: 40px;\r\n    display: flex;\r\n    font-size: 14px;\r\n  }\r\n  .check-content {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-left: 10px;\r\n    width: 60px;\r\n  }\r\n  .check-button {\r\n    width: 20px;\r\n    height: 20px;\r\n    line-height: 20px;\r\n    margin-right: 5px;\r\n  }\r\n  \r\n  .price {\r\n    margin-left: 20px;\r\n    flex: 1;\r\n  }\r\n  .calculate {\r\n    width: 90px;\r\n    background-color: red;\r\n    color: #fff;\r\n    text-align: center;\r\n  }\r\n</style>"],"sourceRoot":"src/views/cart/childComps"}]}